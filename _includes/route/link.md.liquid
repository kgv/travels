{%- assign id = site.data.strings[site.locale][include.toponym.type] | capitalize -%}
{%- if include.toponym.name %}
    {% capture id %}{{ id }} -- {{ include.toponym.name }}{% endcapture %}
{% endif -%}
{%- if include.toponym.coordinates %}
    {% capture id %}{{ id }} -- {{ include.toponym.coordinates.latitude }}, {{ include.toponym.coordinates.longitude }}{% endcapture %}
{% endif -%}
[{{ id }}]: {{ include.toponym.link }}
